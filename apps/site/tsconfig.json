{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@jstemplate/ecommerce-ui": [
        "../../packages/ecommerce-ui",
        "../../packages/ecommerce-ui/dist"
      ]
    },
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "../../packages/ui/src/molecules/single-blog/SocialShare.tsx", "lib/utils/recent-viewid.ts"],
  "exclude": ["node_modules"]
}
